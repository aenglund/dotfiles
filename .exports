# Add ~/bin to PATH
export PATH="$HOME/bin:$PATH";

# Make vim the default editor
export EDITOR="vim";

# Enable colored grep output
export GREP_OPTIONS='--color=auto'
export GREP_COLOR='1;33'

# Add date and time to history command
export HISTTIMEFORMAT="%Y-%m-%d %T "

# Color CLI
export CLICOLOR=1

